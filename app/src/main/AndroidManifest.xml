<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.customer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.customer.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.customer.LoginActivity" />
        <activity android:name="com.customer.MainActivity" />
        <activity android:name="com.customer.AddressActivity" />
        <activity android:name="com.customer.ProductListActivity" />
        <activity android:name="com.customer.ProductDetailActivity" />
        <activity android:name="com.customer.PaymentActivity" />
        <activity android:name="com.customer.ThanksActivity" />
        <activity android:name="com.customer.SignUpActivity"></activity>
    </application>

</manifest>